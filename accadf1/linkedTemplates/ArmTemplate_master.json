{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "accadf1"
		},
		"ADX1_servicePrincipalKey": {
			"type": "secureString",
			"metadata": "Secure string for 'servicePrincipalKey' of 'ADX1'"
		},
		"accadlsinput_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'accadlsinput'"
		},
		"accdeltaoutput_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'accdeltaoutput'"
		},
		"accoutput_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'accoutput'"
		},
		"iotcentralblob1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'iotcentralblob1'"
		},
		"sqlb1input_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'sqlb1input'"
		},
		"synapseStorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'synapseStorage'"
		},
		"waginput1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'waginput1'"
		},
		"ADX1_properties_typeProperties_endpoint": {
			"type": "string",
			"defaultValue": "https://iiotstore.eastus2.kusto.windows.net"
		},
		"ADX1_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "72f988bf-86f1-41af-91ab-2d7cd011db47"
		},
		"ADX1_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "da4ee700-880e-4026-b700-5d77491cdfb5"
		},
		"ADX1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "jemtsdb"
		},
		"accadlsinput_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://accbbstore.dfs.core.windows.net"
		},
		"accoutput_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://accbbstore.dfs.core.windows.net"
		},
		"iotcentralblob1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://iotcentralstore.dfs.core.windows.net"
		},
		"synapseStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://accsynapsestorage.dfs.core.windows.net"
		},
		"waginput1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://waginput.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "accadf1_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADX1_servicePrincipalKey": {
						"value": "[parameters('ADX1_servicePrincipalKey')]"
					},
					"accdeltaoutput_connectionString": {
						"value": "[parameters('accdeltaoutput_connectionString')]"
					},
					"iotcentralblob1_accountKey": {
						"value": "[parameters('iotcentralblob1_accountKey')]"
					},
					"synapseStorage_accountKey": {
						"value": "[parameters('synapseStorage_accountKey')]"
					},
					"waginput1_accountKey": {
						"value": "[parameters('waginput1_accountKey')]"
					},
					"accadlsinput_accountKey": {
						"value": "[parameters('accadlsinput_accountKey')]"
					},
					"accoutput_accountKey": {
						"value": "[parameters('accoutput_accountKey')]"
					},
					"sqlb1input_connectionString": {
						"value": "[parameters('sqlb1input_connectionString')]"
					},
					"ADX1_properties_typeProperties_endpoint": {
						"value": "[parameters('ADX1_properties_typeProperties_endpoint')]"
					},
					"ADX1_properties_typeProperties_tenant": {
						"value": "[parameters('ADX1_properties_typeProperties_tenant')]"
					},
					"ADX1_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('ADX1_properties_typeProperties_servicePrincipalId')]"
					},
					"ADX1_properties_typeProperties_database": {
						"value": "[parameters('ADX1_properties_typeProperties_database')]"
					},
					"iotcentralblob1_properties_typeProperties_url": {
						"value": "[parameters('iotcentralblob1_properties_typeProperties_url')]"
					},
					"synapseStorage_properties_typeProperties_url": {
						"value": "[parameters('synapseStorage_properties_typeProperties_url')]"
					},
					"waginput1_properties_typeProperties_url": {
						"value": "[parameters('waginput1_properties_typeProperties_url')]"
					},
					"accadlsinput_properties_typeProperties_url": {
						"value": "[parameters('accadlsinput_properties_typeProperties_url')]"
					},
					"accoutput_properties_typeProperties_url": {
						"value": "[parameters('accoutput_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "accadf1_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'accadf1_ArmTemplate_0')]"
			]
		},
		{
			"name": "accadf1_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'accadf1_ArmTemplate_1')]"
			]
		}
	]
}